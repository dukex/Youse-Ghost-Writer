{
  "manifest_version": 2,

  "name": "Youse Ghost Writer",
  "description": "Preenche os campos nas p√°ginas da Youse",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Swish and flick!"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
